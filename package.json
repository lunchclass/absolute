{
  "babel": {
    "presets": [
      "es2015",
      "es2017"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-es2017": "^6.0.15",
    "css-loader": "^0.28.4",
    "eslint-config-google": "^0.7.1",
    "eslint-plugin-import": "^2.3.0",
    "file-loader": "^0.11.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "^3.0.1",
    "gulp-load-plugins": "^1.2.4",
    "gulp-mocha": "^4.3.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-sourcemaps": "^1.6.0",
    "mocha": "^3.4.1",
    "node-sass": "^4.5.3",
    "run-sequence": "^1.0.1",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.1"
  },
  "dependencies": {
    "express": "~4.15.2",
    "body-parser": "~1.16.1",
    "mongoose": "^4.10.5",
    "uuid": "~3.0.1",
    "web-push": "~3.2.2"
  },
  "engines": {
    "node": ">=0.12"
  },
  "private": true,
  "eslintConfig": {
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "google",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "rules": {
      "no-console": 0,
      "import/no-commonjs": [2, "allow-primitive-modules"],
      "indent": ["error", 2],
      "sort-imports": ["error", {
        "memberSyntaxSortOrder": ["single", "multiple", "all", "none"]
      }]
    }
  }
}
